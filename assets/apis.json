{
  "provider": "docusign",
  "name": "DocuSign API",
  "description": "DocuSign API",
  "shortDescription": "DocuSign API",
  "labels": {
    "example": "true"
  },
  "api": {
    "targetUrl": "https://demo.docusign.net/restapi/v2/accounts/<provide account id>",
    "specificationUrl": "https://raw.githubusercontent.com/docusign/eSign-OpenAPI-Specification/master/esignature.rest.swagger-v2.json",
    "requestParameters": {
      "headers": {
        "X-DocuSign-Authentication": [
          "{\"Username\":\"<provide username>\",\"Password\":\"<provide password>\",\"IntegratorKey\": \"<provide docusign integration key>\"}"
        ]
      }
    }
  }
}
